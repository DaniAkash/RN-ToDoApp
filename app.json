{
  "expo": {
    "name": "ToDoApp",
    "slug": "ToDoApp",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "userInterfaceStyle": "automatic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "A Todo app built with Dark Mode Support for the Building Mobile Apps with React Native course.",
    "githubUrl": "https://github.com/DaniAkash/RN-ToDoApp"
  }
}
